&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span> self&lt;span style="color:#ff79c6">.&lt;/span>libs[libIndex]&lt;span style="color:#ff79c6">.&lt;/span>registeredDay &lt;span style="color:#ff79c6">=&lt;/span> t
